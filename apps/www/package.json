{
  "name": "www",
  "version": "0.0.1",
  "description": "main website",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "pnpm run content:build && next dev --port 3000",
    "dev:secrets:pull": "AWS_PROFILE=paybill-dev node ./scripts/getSecrets.js -n local/www",
    "build": "pnpm run content:build && next build && ./scripts/upload-static-assets.sh",
    "start": "next start",
    "lint": "eslint .",
    "test": "vitest --run --coverage",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:update": "vitest --run --update",
    "test:ci": "vitest --run --coverage",
    "test:report": "open coverage/lcov-report/index.html",
    "clean": "rimraf tsconfig.tsbuildinfo node_modules .next .contentlayer .turbo",
    "pretypecheck": "next typegen",
    "typecheck": "pnpm run content:build && tsc --noEmit",
    "content:build": "contentlayer2 build && node scripts/generateStaticContent.mjs",
    "prettier:check": "prettier --cache --check '{pages,components,lib,styles,tests}/**/*.{js,jsx,ts,tsx,css,md,mdx,json}'",
    "prettier:write": "prettier --cache --write \"./{pages,components,lib,styles,tests}/**/*.{ts,tsx,md,js,jsx,json}\"",
    "postbuild": "node ./scripts/generate-sitemap.mjs && ./scripts/upload-static-assets.sh"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.410.0",
    "@code-hike/mdx": "^0.9.0",
    "@heroicons/react": "^1.0.6",
    "common": "workspace:*",
    "ui": "workspace:*",
    "platform-api-types": "workspace:*",
    "@mdx-js/loader": "^2.1.5",
    "@mdx-js/react": "^2.3.0",
    "@next/bundle-analyzer": "15.3.1",
    "@next/mdx": "15.3.1",
    "@usercentrics/cmp-browser-sdk": "^4.42.0",
    "@tanstack/react-query": "4.35.7",
    "@tanstack/react-query-devtools": "4.35.7",
    "@sentry/nextjs": "^10.3.0",
    "classnames": "^2.3.1",
    "contentlayer2": "^0.5.8",
    "dat.gui": "^0.7.9",
    "sonner": "^1.5.0",
    "dayjs": "^1.11.12",
    "react-error-boundary": "^4.0.13",
    "framer-motion": "^11.0.3",
    "globby": "^13.2.2",
    "gray-matter": "^4.0.3",
    "lodash": "^4.0.0",
    "lucide-react": "^0.436.0",
    "markdown-toc": "^1.2.0",
    "next": "catalog:",
    "next-contentlayer2": "^0.5.8",
    "next-mdx-remote": "^4.4.1",
    "next-seo": "^6.5.0",
    "next-themes": "^0.3.0",
    "prettier": "^3.6.2",
    "path-to-regexp": "^8.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^8.0.3",
    "react-use": "^17.4.0",
    "recharts": "^2.8.0",
    "rehype-slug": "^5.1.0",
    "remark": "^15.0.1",
    "remark-gfm": "^3.0.1",
    "remark-html": "^16.0.1",
    "sass": "^1.89.2",
    "swiper": "^11.0.7",
    "tailwindcss": "catalog:",
    "tailwindcss-animate": "^1.0.6",
    "tailwindcss-radix": "^2.0.0",
    "typed.js": "^2.0.16",
    "typescript": "~5.5.0",
    "valtio": "^2.1.5",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.6.0",
    "@testing-library/react": "^16.0.0",
    "@types/dat.gui": "^0.7.12",
    "@tailwindcss/container-queries": "^0.1.1",
    "@types/lodash": "^4.0.0",
    "@types/mdx-js__react": "^1.5.6",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint-config-next": "15.3.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^2.0.4",
    "eslint-plugin-barrel-files": "^2.0.7",
    "next-router-mock": "^0.9.13",
    "jsdom-testing-mocks": "^1.13.1",
    "msw": "^2.3.0",
    "nuqs": "2.7.1",
    "node-mocks-http": "^1.17.2",
    "postcss": "^8.5.3",
    "react-hook-form": "^7.45.0",
    "rimraf": "^6.0.0",
    "uuid": "^9.0.1",
    "axios": "^1.12.0",
    "zod": "^3.22.4",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "catalog:"
  }
}
